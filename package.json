{
  "name": "fcmp-by-q4-2019-reactjs",
  "version": "1.0.0",
  "description": "Part 1",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --output-public-path=/dist/  --mode development --progress --open --hot",
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:reporter": "jest --reporters",
    "debug": "set NODE_ENV=debug npm test"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "scss"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "reporters": [
      "default"
    ],
    "preset": "jest-puppeteer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliaksandrKazerski/FCMP-BY-Q4-2019-ReactJS.git"
